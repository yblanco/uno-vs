{
  "name": "uno-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon ./bin/www",
    "migrate:create": "migrate create -d mongodb://localhost/uno_vs",
    "migrate:up": "migrate up -d mongodb://localhost/uno_vs --autosync",
    "migrate:down": "migrate down -d mongodb://localhost/uno_vs",
    "migrate": "migrate up --autosync"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.3.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "migrate-mongoose": "^4.0.0",
    "mongoose": "^5.9.7",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.2",
    "request-ip": "^2.1.3",
    "socket.io-client": "^2.3.0",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.4.2"
  }
}
